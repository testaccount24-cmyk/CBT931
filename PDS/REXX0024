 /* REXX PROGRAM REXX0024 TO ILLUSTRATE INDEX() */
 ALPHABET = "ABCDEFGHIJKLMNOPQRSTUVWXYZ"
 SAY INDEX(ALPHABET,"G",1)   /* GIVES 7 */

 ALPHABET = "ABCDEFGHIJKLMNOPQRSTUVWXYZ"
 SAY INDEX(ALPHABET,"1",1)   /* NOT FOUND. GIVES 0*/

 ALPHABET = "ABCDEFGHIJKLMNOPQRSTUVWXYZ"
 SAY INDEX(ALPHABET,"G",27)   /* GIVES 0 */
