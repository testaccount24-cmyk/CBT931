/* REXX0017 TO ILLUSTRATE LITERALS AND MATH OPERATIONS */
MYVAR = "1 + 2"               /* NO MATH DONE */
SAY MYVAR                     /* 1 + 2 */
VAR1 = MYVAR                  /* THIS DOES THE MATH */
SAY VAR1                      /* 3 */
/* NO REXX EQUIVALENT OF CLIST SET VAR2   = &STR(&MYVAR) */
