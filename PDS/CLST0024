 /* CLST0024 TO ILLUSTRATE &SYSINDEX */
 SET ALPHABET = ABCDEFGHIJKLMNOPQRSTUVWXYZ
 WRITE &SYSINDEX(G,&ALPHABET,1)   /* GIVES 7 */

 SET ALPHABET = ABCDEFGHIJKLMNOPQRSTUVWXYZ
 WRITE &SYSINDEX(1,&ALPHABET,1)   /* NOT FOUND. GIVES 0*/

 SET ALPHABET = ABCDEFGHIJKLMNOPQRSTUVWXYZ
 WRITE &SYSINDEX(G,&ALPHABET,27)  /* GIVES 0 */
