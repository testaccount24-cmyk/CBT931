/* CLST0040 DO INCREMENTING A VARIABLE TO A LIMIT */
WRITE &STR(INDEX = 1 TO 20 BY 1)
DO INDEX = 1 TO 20 BY 1
    WRITE &INDEX
END

WRITE
WRITE &STR(INDEX = 1 TO 20 BY 2)
DO INDEX = 1 TO 20 BY 2
    WRITE &INDEX
END

WRITE
WRITE &STR(INDEX = 10 TO -2 BY -1)
DO INDEX = 10 TO -2 BY -1
    WRITE &INDEX
END
