/* REXX REXX0002 TO SHOW HOW IT HANDLES SLASHES IN DATA */
 NAME = "JOE" /* NORMAL WAY TO SPECIFY A LITERAL          */
 NAME = JOE   /* JOE IS UPPERCASED AND TAKEN AS A LITERAL.*/
 BAD_DATE  = 09/06/15   /* DIVISION IS ATTEMPTED = 0.1 */
 SAY BAD_DATE
 GOOD_DATE = "09/06/15" /* NO DIVISION IS DONE   */
 SAY GOOD_DATE
