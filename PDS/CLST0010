/* CLST0010 TO TEST &LASTCC AND &MAXCC */
ERROR DO
    WRITE &LASTCC  &MAXCC
    WRITE IN ERROR ROUTINE
    WRITE &LASTCC  &MAXCC
    RETURN /* IGNORE ERROR, GO BACK AFTER OFFENDING STATEMENT */
END

WRITE BEFORE DOING ANYTHING &LASTCC
WRITE ABOUT TO EXECUTE BAD CLIST STATEMENT
SET TOTAL = 1 + B
WRITE AFTER BAD CLIST STATEMENT &LASTCC  &MAXCC

