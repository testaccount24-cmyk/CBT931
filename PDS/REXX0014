/* REXX REXX0014 TO TEST THE LENGTH FUNCTION */
SAY LENGTH(ABCD)        /* 4 */
SAY LENGTH(1 + 2)       /* 1 */
VAR = 1 + 2
SAY LENGTH(VAR)         /* 1 WORKS THE SAME WITH VARIABLE */
SAY LENGTH("1 + 2")     /* 5 */
SAY LENGTH(      ABCD)  /* 4 */
SAY LENGTH("    ABCD")  /* 8 */
SAY LENGTH(ABCD      )  /* 4 */
SAY LENGTH("ABCD    ")  /* 8 */
