/* REXX REXX0012 TO TEST DATATYPE FUNCTION */
SAY DATATYPE(ABCD) "ABCD"    /* CHAR */
SAY DATATYPE(1234) "1234"    /* NUM */
SAY DATATYPE(1 + 1) "1 + 1"  /* NUM */
SAY DATATYPE(10.20) "10.20"  /* NUM */
SAY DATATYPE(DSN.ABCD.DATA) "DSN.ABCD.DATA" /*CHAR*/
SAY DATATYPE(ABCD1234) "ABCD1234" /* CHAR */
