 /* CLST0027 TO ILLUSTRATE DO - END SEQUENCES */
 SET SALARY = 100
 WRITE PLEASE ENTER NAME (JOE)
 READ NAME
 IF &NAME = JOE +
 THEN DO /* NAME = JOE */
     SET SALARY = &SALARY * 2
     WRITE DOUBLING JOE'S SALARY
     WRITE NEW SALARY IS &SALARY
     END /* NAME = JOE */
 ELSE DO /* NAME NE  JOE */
     SET  SALARY = &SALARY - 1
     WRITE REDUCING SALARY BY ONE
     WRITE NEW SALARY IS &SALARY
     END /* NAME NE  JOE */

