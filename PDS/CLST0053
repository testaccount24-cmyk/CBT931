/* CLST0053 TO ILLUSTRATE WRITING THREE RECORDS */
CONTROL LIST CONLIST
ALLOCATE DDN(OUTFILE) OLD REUSE DSN('GABRIEL.TEST2.DATA')
OPENFILE OUTFILE OUTPUT
SET OUTFILE = RECORD1 WILL BE WRITTEN
PUTFILE OUTFILE
SET OUTFILE = RECORD2 WILL BE WRITTEN
PUTFILE OUTFILE
SET OUTFILE = RECORD3 WILL BE WRITTEN
PUTFILE OUTFILE

CLOSFILE OUTFILE
FREE DDN(OUTFILE)
