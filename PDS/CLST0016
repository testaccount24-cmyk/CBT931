/* CLST0016 TO ILLUSTRATE THE &SYSNSUB FUNCTION */
SET LEVEL1 = JOE
SET LEVEL2 =  &&LEVEL1 /* FIRST  LEVEL VARIABLE IS PUT IN &LEVEL2 */
SET LEVEL3 =  &&LEVEL2 /* SECOND LEVEL VARIABLE IS PUT IN &LEVEL3 */
WRITE &LEVEL3 /* GIVES "JOE" */
WRITE &SYSNSUB(0,&LEVEL3) /* GIVES "&LEVEL3" */
WRITE &SYSNSUB(1,&LEVEL3) /* GIVES "&LEVEL2" */
WRITE &SYSNSUB(2,&LEVEL3) /* GIVES "&LEVEL1" */
WRITE &SYSNSUB(3,&LEVEL3) /* GIVES "JOE"     */
