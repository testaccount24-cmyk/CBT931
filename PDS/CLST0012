/* CLST0012 TO TEST FUNCTION &DATATYPE */
WRITE &DATATYPE(ABCD) &STR(ABCD)                   /*CHAR*/
WRITE &DATATYPE(1234) &STR(1234)                   /*NUM*/
WRITE &DATATYPE(1 + 1) &STR(1 + 1)                 /*NUM*/
WRITE &DATATYPE(10.20) &STR(10.20)                 /*CHAR*/
WRITE &DATATYPE(DSN.ABCD.DATA) &STR(DSN.ABCD.DATA) /*CHAR*/
WRITE &DATATYPE(ABCD1234) &STR(ABCD1234)           /*CHAR*/
